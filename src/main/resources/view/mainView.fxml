<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.media.MediaView?>
<?import javafx.scene.web.WebView?>

<AnchorPane prefHeight="800.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.hackathon.controller.MainViewController">
   <children>
      <MediaView fx:id="video" fitHeight="600.0" fitWidth="800.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="150.0" AnchorPane.topAnchor="0.0" />
      <WebView fx:id="data" prefHeight="200.0" prefWidth="200.0" visible="false" AnchorPane.bottomAnchor="62.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" />
      <Label fx:id="progress" text="Label" AnchorPane.bottomAnchor="40.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" />
      <ButtonBar layoutX="6.0" layoutY="760.0" prefHeight="40.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0">
        <buttons>
            <Button fx:id="btPlay" mnemonicParsing="false" onAction="#methodPlay" text="play" />
            <Button fx:id="btPause" mnemonicParsing="false" onAction="#methodPause" text="pause" />
            <Button fx:id="btStop" mnemonicParsing="false" onAction="#methodStop" text="stop" />
          <Button fx:id="btNext" mnemonicParsing="false" onAction="#methodNext" text="next" />
            <Button mnemonicParsing="false" onAction="#getData" text="Button" visible="false" />
        </buttons>
      </ButtonBar>
   </children>
</AnchorPane>
